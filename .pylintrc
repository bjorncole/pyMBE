[FORMAT]
max-line-length = 99

[MESSAGES CONTROL]
disable=attribute-defined-outside-init,
        cyclic-import,
        duplicate-code,
        fixme,
        missing-class-docstring,
        missing-function-docstring,
        missing-module-docstring,
        missing-timeout,
        protected-access,
        too-few-public-methods,
        too-many-ancestors,
        too-many-arguments,
        too-many-branches,
        too-many-function-args,
        too-many-locals,
        too-many-nested-blocks,
        unused-argument,
        use-dict-literal,

[REPORTS]
output-format=colorized
max-nested-blocks=5
